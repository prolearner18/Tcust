<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />

	<title>校園日誌管理</title>


	<link rel="stylesheet" href="~/css/admin/bootstrap.min.css" />
	<link rel="stylesheet" href="~/libs/fontawesome/css/font-awesome.min.css" />
	<link rel="stylesheet" href="~/css/admin/admin.css" />
</head>
<body>
	<div id="header">
		<nav class="navbar navbar-inverse navbar-fixed-top">
			<div class="container">
				<div class="navbar-header">
					<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
					<a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand" style="font-size:1.8em">校園日誌管理</a>
				</div>
				<div class="navbar-collapse collapse">
					<ul class="nav navbar-nav">
						
					</ul>
				</div>
			</div>
		</nav>
	</div>
	<div id="main" class="container body-content">
		@RenderBody()
		<hr />
		<footer>
			<p>&copy; 2017 - 慈濟科大電算中心</p>
		</footer>
	</div>



	<script type="text/javascript" src="~/libs/jquery.min.js"></script>
	<script type="text/javascript" src="~/libs/tether.min.js"></script>
	<script type="text/javascript" src="~/libs/bootstrap.min.js"></script>

	<script type="text/javascript" src="~/js/app.js"></script>

	@RenderSection("Scripts", required: false)
