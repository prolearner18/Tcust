
<manage :init_model="model" :permissions="permissions" ></manage>


@section scripts{
	<script>
		$("#linkManage").addClass("active");

		new Vue({
			el: '#main',
			data() {
				return {
					model: {},
					permissions: [],
					
				}
			},
			beforeMount() {
				this.model =@Html.Raw(ViewData["list"]);

				let permissionList =@Html.Raw(ViewData["permissions"]);
				this.permissions = permissionList;

				

				let token = '@Html.Raw(ViewData["token"])';
				this.$auth.setToken(token);
			},
			methods: {

			}

		});



	</script>

}
