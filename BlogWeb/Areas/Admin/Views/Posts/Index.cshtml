<post-admin :init_model="model" :can_delete="can_delete" :categories="categories"></post-admin>



@section scripts{
	<script>
		$("#linkPosts").addClass("active");

		new Vue({
			el: '#main',
			data() {
				return {
					model: {},
					can_delete:0,
					categories: []
				}
			},
			beforeMount() {
				this.model =@Html.Raw(ViewData["list"]);

				let can_delete=@Html.Raw(ViewData["can_delete"]);
				this.can_delete = Helper.isTrue(can_delete);

				let categoryList =@Html.Raw(ViewData["categories"]);
				this.categories = categoryList;
			},
			methods: {

			}

		});



	</script>

}



