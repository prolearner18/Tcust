<post-admin :init_model="model" :categories="categories"></post-admin>



@section scripts{
	<script>
		new Vue({
			el: '#main',
			data() {
				return {
					model: {},
					categories: []
				}
			},
			beforeMount() {
				this.model =@Html.Raw(ViewData["list"]);

				let categoryList =@Html.Raw(ViewData["categories"]);
				this.categories = categoryList;
			},
			methods: {

			}

		});


		
	</script>

}



